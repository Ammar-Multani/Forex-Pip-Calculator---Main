{
  "name": "Pip Calc",
  "displayName": "Pip Calc",
  "expo": {
    "name": "Pip Calc",
    "slug": "pip-calc",
    "scheme": "pipcalc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "jsEngine": "hermes",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.enacton.pipcalc",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "UIBackgroundModes": [],
        "NSCameraUsageDescription": "This app does not use your camera",
        "NSPhotoLibraryUsageDescription": "This app does not access your photos",
        "NSPhotoLibraryAddUsageDescription": "This app does not save photos to your library",
        "NSMicrophoneUsageDescription": "This app does not use your microphone",
        "NSLocationWhenInUseUsageDescription": "This app does not use your location",
        "ITSAppUsesNonExemptEncryption": false
      },
      "appStoreUrl": "https://apps.apple.com/app/YOUR_APP_ID",
      "usesIcloudStorage": false,
      "config": {
        "usesNonExemptEncryption": false
      },
      "privacyManifests": {
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": ["CA92.1"]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryFileTimestamp",
            "NSPrivacyAccessedAPITypeReasons": ["C617.1"]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryDiskSpace",
            "NSPrivacyAccessedAPITypeReasons": ["C617.1"]
          }
        ]
      }
    },
    "android": {
      "icon": "./assets/icon.png",
      "package": "com.enacton.pipcalc",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-light.png",
        "monochromeImage": "./assets/adaptive-icon-dark.png",
        "backgroundColor": "#ffffff"
      },
      "allowBackup": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash-icon-light.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ecedee",
          "dark": {
            "image": "./assets/splash-icon-dark.png",
            "backgroundColor": "#151718"
          }
        }
      ]
    ],
    "extra": {
      "ALPHA_VANTAGE_API_KEY": "${EXPO_PUBLIC_ALPHA_VANTAGE_API_KEY}",
      "FIXER_API_KEY": "${EXPO_PUBLIC_FIXER_API_KEY}",
      "CURRENCY_LAYER_API_KEY": "${EXPO_PUBLIC_CURRENCY_LAYER_API_KEY}",
      "EXCHANGE_RATE_API_KEY": "${EXPO_PUBLIC_EXCHANGE_RATE_API_KEY}",
      "TRADER_MADE_API_KEY": "${EXPO_PUBLIC_TRADER_MADE_API_KEY}",
      "API_PRIORITY": "${EXPO_PUBLIC_API_PRIORITY}",
      "NODE_ENV": "${NODE_ENV}"
    },
    "eas": {
      "projectId": "11aba8bb-8399-4c4b-b8dd-1c547005748f"
    }
  }
}
